    "compile": "tsc -p ./",
    "prepare-release": "tsc -p ./",
    "build:native": "tsx scripts/build-native.ts",
    "test": "npm run build:native && npm run compile && mocha -r ts-node/register test/**/*.test.ts"
  },
  "dependencies": {
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.3.0",
    "mocha": "^11.7.5",
    "node-gyp": "^12.2.0",
    "prebuildify": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
